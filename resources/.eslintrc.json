{
	"root": true,
	"plugins": [
		"no-jquery"
	],
	"extends": [
		"plugin:no-jquery/deprecated",
		"plugin:no-jquery/slim",
		"wikimedia/client",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"env": {
		"browser": true
	},
	"globals": {
		"videojs": true
	},
	"parserOptions": {
		"sourceType": "module"
	},
	"rules": {
		"jsdoc/no-undefined-types": "off",
		"no-restricted-properties": "warn",
		"compat/compat": "warn",
		"es-x/no-array-prototype-fill": "warn",
		"no-jquery/no-extend": "warn",
		"no-jquery/no-done-fail": "warn"
	},
	"overrides": [
		{
			"files": [
				"lib/foreign-resources.yaml"
			],
			"rules": {
				"yml/no-empty-mapping-value": "off"
			}
		}
	]
}
